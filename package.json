{
  "name": "corvicac-web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000 --hostname 0.0.0.0",
    "clean": "rimraf ./.next ./node_modules",
    "build": "cross-env NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "build:analyze": "NODE_ENV=production ANALYZE=true next build",
    "hostinger:build": "next build",
    "hostinger:deploy": "export NODE_ENV=production && next build",
    "hardened:build": "NODE_ENV=production NEXT_DISABLE_SWC_CACHE=1 next build",
    "dev:server": "node server.js",
    "start:server": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "aos": "2.3.4",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "framer-motion": "11.11.12",
    "helmet": "^8.1.0",
    "next": "14.2.35",
    "postcss": "8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.2.1",
    "sharp": "0.33.5",
    "autoprefixer": "10.4.19",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@types/aos": "3.0.7",
    "@types/node": "20.19.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.35",
    "rimraf": "3.0.2",
    "typescript": "5.7.2"
  },
  "overrides": {
    "glob": "8.1.0",
    "minimatch": "5.1.6",
    "lru-cache": "7.18.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "next": {
    "swcMinify": true,
    "output": "standalone"
  }
}
